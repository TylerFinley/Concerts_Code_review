<h1>Musicians</h1>
<h3>Add your band</h3>

<form id="form_id" action="/band/new" method="post">
  <input type="text" name="band" placeholder="band name">
  <button name="submit">Submit</button>

</form>
<ul>
  <% @band.each do |band|%>
  <li><a href="/band/<%= band.id %>"><%= band.band.capitalize() %></a></li>

  <%end%>

</ul>
